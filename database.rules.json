        ".write":"$user_id === auth.uid"
      }
    },
      "rooms":{
        ".read" :"auth!== null",
        ".write":"auth!==null",
      "$rooms_id":{
        ".read" :"auth!== null",
        ".write":"auth!==null"
      }
      },
      "messages":{
        ".read" :"auth!== null",
        ".write":"auth!==null",
      "$messages_id":{
        ".read" :"auth!== null",
        ".write":"auth!==null"
      }
      },
    ".read": false,
    ".write": false
  }
}